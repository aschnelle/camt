    "require": {
        "php": "^7.0",
        "ext-simplexml": "*",
        "ext-libxml": "*",
        "ext-dom": "*",
        "jschaedl/iban": "~1.1.0",
        "moneyphp/money": "~3.1.3"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.11",
        "phpunit/phpunit": "~6"
    },
    "autoload": {
        "psr-4": {
            "Genkgo\\Camt\\": ["src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Genkgo\\TestCamt\\": ["test"]
        }
    }
}
